<%= simple_form_for(@supplier) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :website %>
    <%= f.input :account_number %>
    <%= f.input :sales_contact %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
